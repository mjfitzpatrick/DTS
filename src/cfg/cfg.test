#
#  DTS Test Configuration
#


verbose   0
debug     0
monitor	  tucana.tuc.noao.edu:2999
semId	  1024
hb_time	  5

password  dtsPass			# test password


########################################
# DTS dev/test systems
########################################

dts
    name      tucana			# Required fields
    host      tucana.tuc.noao.edu
    port      3000
    loPort    3005
    hiPort    3100
    root      /tmp/dts.tuc/
    contact   3001
    network   dev

    dbfile    /tmp/dts.tuc/msg.db	# Optional field
    logfile   /tmp/dts.log

    # /////////////////////////////////////////////
    # Queue configurations
    # /////////////////////////////////////////////
    queue 				# entry point from domes
	name	      	test		# queue name
        node          	endpoint	# ingest, transfer, or endpoint
        type          	normal		# normal, scheduled, priority
	mode	        push		# push or give
	method        	dts		# dts or ????
	nthreads      	10		# No. of outbound threads
        port	      	4007		# base transfer port
        keepalive	yes		# keep connection open?		 [NYI]
        purge   	yes		# purge complete spool
        #src		crux		# 
        src		tucana2		# 
	#deliverAs   	$D
	deliveryDir   	/tmp/foo.tuc	# spool or /path/to/copy
	#deliveryCmd	/tmp/dlog $F $D test
	#deliveryCmd	/usr/local/bin/imdisp $F

    queue 				# entry point from domes
	name	      	udt		# queue name
        node          	endpoint	# ingest, transfer, or endpoint
        type          	normal		# normal, scheduled, priority
	mode	        push		# push or give
	method        	udt		# dts or ????
	nthreads      	1		# No. of outbound threads
        port	      	3007		# base transfer port
        keepalive	yes		# keep connection open?		 [NYI]
        purge   	yes		# purge complete spool
        src		tucana2		# 
	deliveryDir   	/tmp/foo.tuc	# spool or /path/to/copy


